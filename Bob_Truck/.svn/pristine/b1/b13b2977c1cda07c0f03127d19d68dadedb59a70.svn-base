local ModTechListPanelBinder = class("ModTechListPanelBinder");

function ModTechListPanelBinder:ctor(gameObject)
	local transform = gameObject.transform;
	self.m_bkgnd = transform:Find("m_bkgnd"):GetComponent("UITexture");
	self.m_btnClose = transform:Find("Anchor_Top/m_btnClose"):GetComponent("UIButton");
end
return ModTechListPanelBinder