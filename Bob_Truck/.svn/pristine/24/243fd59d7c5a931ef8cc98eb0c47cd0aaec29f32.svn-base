local TabItem = require("Game/Module/UICommon/TabItem")

local FleetPortTabItem0 = register("FleetPortTabItem0", TabItem)

function FleetPortTabItem0:Awake(gameObject)
    FleetPortTabItem0.super.Awake(self, gameObject)

    self.view = {}
    self.view.n_3DModelDisplay = self:FindComponent("n_3DModelDisplay", "UISprite")
    self.view.n_Des01 = self:FindComponent("AttrNum/n_Des01", "UILabel")
    self.view.n_AttrNum01 = self:FindComponent("AttrNum/n_Des01/n_AttrNum01", "UILabel")
    self.view.n_Des02 = self:FindComponent("AttrNum/n_Des02", "UILabel")
    self.view.n_AttrNum02 = self:FindComponent("AttrNum/n_Des02/n_AttrNum02", "UILabel")
    self.view.n_Des03 = self:FindComponent("AttrNum/n_Des03", "UILabel")
    self.view.n_AttrNum03 = self:FindComponent("AttrNum/n_Des03/n_AttrNum03", "UILabel")
    self.n_Des04 = self:FindComponent("AttrNum/n_Des04", "UILabel")
    self.view.n_AttrNum04 = self:FindComponent("AttrNum/n_Des04/n_AttrNum04", "UILabel")
    self.view.n_Des05 = self:FindComponent("AttrNum/n_Des05", "UILabel")
    self.view.n_AttrNum05 = self:FindComponent("AttrNum/n_Des05/n_AttrNum05", "UILabel")
    self.view.n_FeetName = self:FindComponent("FeetTitel/n_FeetName", "UILabel")
    self.view.n_FeetPower = self:FindComponent("FeetTitel/n_FeetPower", "UILabel")
    self.view.n_ProgressRate = self:FindComponent("g1/n_ProgressRate", "UILabel")
    self.view.n_BgFron = self:FindComponent("g1/n_ProgressRate/n_BgFron", "UISprite")
    self.view.n_FuelSupply = self:FindComponent("g1/n_FuelSupply", "UILabel")
    self.view.n_Discharge = self:FindComponent("g1/n_Discharge", "UILabel")
    self.view.n_Disband = self:FindComponent("n_Disband", "UILabel")

    self:InitView()

    self.planetaryProxy = Facade:RetrieveProxy(NotiConst.Proxy_Planetary)
    self.storehouseProxy = Facade:RetrieveProxy(NotiConst.Proxy_Storehouse)

    Facade:SendNotification(NotiConst.Notify_FleetProtInitTabItemView, {key = "FleetPortTabItem0", view = self.view})
end

function FleetPortTabItem0:InitView()
    local NLuaClickEvent = NLuaClickEvent.Get(self.view.n_Disband.gameObject)
    NLuaClickEvent:AddClick(self, FleetPortTabItem0.DisbandClick)

    NLuaClickEvent = NLuaClickEvent.Get(self.view.n_FuelSupply.gameObject)
    NLuaClickEvent:AddClick(self, FleetPortTabItem0.FuelSupplyClick)

    NLuaClickEvent = NLuaClickEvent.Get(self.view.n_Discharge.gameObject)
    NLuaClickEvent:AddClick(self, FleetPortTabItem0.LoadGoodsClick)
end

--解散舰艇
function FleetPortTabItem0:DisbandClick()
    Facade:ReplacePanel("FleetDisbandPanel")
end

--合并舰艇
function FleetPortTabItem0:FuelSupplyClick()
    
end

--装卸货物
function FleetPortTabItem0:LoadGoodsClick()
    Facade:ReplacePanel("FleetGoodsPanel")
end

return FleetPortTabItem0